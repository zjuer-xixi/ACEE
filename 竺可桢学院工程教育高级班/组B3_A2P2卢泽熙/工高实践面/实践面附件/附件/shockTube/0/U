FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1.7 0 0);

boundaryField
{
    sides_in
    {
        type            inletOutlet;
        inletValue      uniform (2 0 0);
        value           uniform (2 0 0);
    }
    sides_out
    {
        type            zeroGradient;
    }
    empty
    {
        type            empty;
    }
}


