FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0.85;

boundaryField
{
    sides_in
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           uniform 1;
    }
    sides_out
    {
        /*
        type            inletOutlet;
        inletValue      uniform 0.05;
        value           uniform 0.05;
        */
        type            fixedValue;
        value           uniform 0.7;
    }
    empty
    {
        type            empty;
    }
}


