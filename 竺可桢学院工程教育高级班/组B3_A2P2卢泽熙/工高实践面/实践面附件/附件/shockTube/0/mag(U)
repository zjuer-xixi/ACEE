FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      mag(U);
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform 1.7;

boundaryField
{
    sides_in
    {
        type            calculated;
        value           uniform 2;
    }
    sides_out
    {
        type            calculated;
        value           uniform 1.7;
    }
    empty
    {
        type            empty;
    }
}


