FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues ( volVectorFieldValue U (1.7 0 0) volScalarFieldValue T 0.5 volScalarFieldValue p 0.175);


regions( 
     boxToCell { box (0 -1 -1) (5 1 1) ; 
		fieldValues (volVectorFieldValue U (0.7 0 0) volScalarFieldValue T 0.8 volScalarFieldValue p 0.56 ) ; 
	}
);



